/**
 * @param {...(null|boolean|number|string|Array|Object)} args
 * @return {number}
 */
var argumentsLength = function(...args) {
    // The function uses rest parameter syntax to collect all arguments into an array called 'args'.
    // It then returns the length of this array.
    return args.length;
};

/**
 * Example usage:
 * console.log(argumentsLength(1, 2, 3)); // 3
 * console.log(argumentsLength({}, null, "3")); // 3
 */
